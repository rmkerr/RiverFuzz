@model List<FuzzerRunViewModel>

@{ 
    ViewData["Title"] = "All Scans";
}

<h1>@ViewData["Title"]</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Start Time</th>
            <th scope="col">End Time</th>
            <th scope="col">Execution Time</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; ++i)
        {
        <tr>
            <th scope="row"><a asp-controller="Results" asp-action="Summary" asp-route-id="@Model[i].id">@Model[i].id</a></th>
            <td>@Model[i].name</td>
            <td>@Model[i].start_time</td>
            <td>@Model[i].end_time</td>
            <td>@(Model[i].end_time - Model[i].start_time)</td>
        </tr>
        }
    </tbody>
</table>