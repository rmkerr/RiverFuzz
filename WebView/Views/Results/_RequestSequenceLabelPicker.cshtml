@model RequestSequenceLabelViewModel

<form asp-action="AddRequestSequenceLabel" asp-controller="Results">
    <input class="form-control" type="hidden" asp-for="sequence_id" />
    <div class="form-group">
        <select class="form-control" asp-for="name" asp-items="RequestSequenceLabelViewModel.Labels"></select>
    </div>
    <button type="submit" class="btn btn-secondary">Submit</button>
</form>