@model List<RequestSequenceViewModel>

@{
    ViewData["Title"] = "Summary";
}

<h1>@ViewData["Title"]</h1>
<ul>
    @for (int i = 0; i < Model.Count; ++i)
    {
        string url = Model[i].Requests[Model[i].Requests.Count - 1].url;
        string result = Model[i].Responses[Model[i].Responses.Count - 1].status;
        <li>
            <a asp-controller="Results" asp-action="Sequence" asp-route-id="@Model[i].id">@Model[i].id - @result - @url</a>
            @foreach (string label in Model[i].Labels)
            {
                <span class="badge badge-primary">@label</span>
            }
        </li>
    }
}
</ul>